<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta property="post" resource="Post"/>
        <meta property="posts" resource="Posts"/>
        <link href="/assets/example.css" media="screen" rel="stylesheet" type="text/css">
    </head>
    <body>

        <h1>{{post.title}}</h1>
        <p>{{post.content}}</p>
        <br>
        <hr>
        <h2><a href="/">All posts</a></h2>
        {{#each posts}}
            <h3><a href="/post?id={{id}}">{{title}}</a></h3>
            <hr>
        {{/each}}
    </body>
</html>
